<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
         version="6.0">

    <!-- start of program servlet lifecycle -->
    <servlet>
        <servlet-name>ServletLifecycle</servlet-name>
        <servlet-class>org.suyash.labservlet.ServletLifecycle</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletLifecycle</servlet-name>
        <url-pattern>/servlet-lifecycle</url-pattern>
    </servlet-mapping>
    <!-- end of program servlet lifecycle -->

    <!-- start of program servlet form -->
    <servlet>
        <servlet-name>FormServlet</servlet-name>
        <servlet-class>org.suyash.labservlet.FormServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FormServlet</servlet-name>
        <url-pattern>/form-servlet</url-pattern>
    </servlet-mapping>
    <!-- end of program servlet form -->
</web-app>